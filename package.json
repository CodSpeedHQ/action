{
  "name": "@codspeed/action",
  "version": "1.6.0",
  "private": true,
  "description": "Github Actions for running CodSpeed in your CI",
  "main": "lib/main.js",
  "scripts": {
    "build": "pnpm build:tsc && pnpm build:package",
    "build:tsc": "tsc --noEmit",
    "build:package": "esbuild --platform=node --bundle --sourcemap src/main.ts --define:process.env.VERSION=\\\"$npm_package_version\\\" --minify --outfile=dist/index.js",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "release": "./scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodSpeedHQ/action.git"
  },
  "keywords": [
    "actions",
    "codspeed",
    "performance",
    "benchmarking"
  ],
  "author": "CodSpeed",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "md5-file": "^5.0.0",
    "node-fetch": "^2.6.9",
    "tar": "^6.1.13"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^6.10.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/node-fetch": "^2.6.3",
    "@types/tar": "^6.1.4",
    "@typescript-eslint/parser": "^4.33.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "0.17.12",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^25.7.0",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
